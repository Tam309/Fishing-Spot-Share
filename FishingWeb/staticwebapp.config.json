{
  "routes": [
    {
      "route": "/login",
      "rewrite": "/index.html"
    },
    {
      "route": "/{*all}",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/images/*", "*.js", "*.css", "*.json", "*.ico", "*.png", "*.jpg", "*.svg"],
    "statusCode": 200
  }
}
